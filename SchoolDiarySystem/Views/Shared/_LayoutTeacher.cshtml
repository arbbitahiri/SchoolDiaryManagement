    <div id="preloader"></div>
    <!-- Preloader End Here -->
    <div id="wrapper" class="wrapper bg-ash" style="display: inline;">
        <!-- Header Menu Area Start Here -->
        <div class="navbar navbar-expand-md header-menu-one bg-light">
            <div class="nav-bar-header-one">
                <div class="header-logo">
                    <a href="@Url.Action("Index", "Professor")">
                        <img src="~/Content/img/favicon.png" alt="logo">
                    </a>
                </div>
                <div class="toggle-button sidebar-toggle">
                    <button type="button" class="item-link">
                        <span class="btn-icon-wrap">
                            <span></span>
                            <span></span>
                            <span></span>
                        </span>
                    </button>
                </div>
            </div>
            <div class="d-md-none mobile-nav-bar">
                <button class="navbar-toggler pulse-animation" type="button" data-toggle="collapse" data-target="#mobile-navbar" aria-expanded="false">
                    <i class="far fa-arrow-alt-circle-down"></i>
                </button>
                <button type="button" class="navbar-toggler sidebar-toggle-mobile">
                    <i class="fas fa-bars"></i>
                </button>
            </div>
            <div class="header-main-menu collapse navbar-collapse" id="mobile-navbar">
                <ul class="navbar-nav">
                    <li class="navbar-item header-search-bar">
                        <div class="input-group stylish-input-group">
                        </div>
                    </li>
                </ul>
                <ul class="navbar-nav">
                    <li class="navbar-item dropdown header-admin">
                        <a class="navbar-nav-link dropdown-toggle" href="#" role="button" data-toggle="dropdown"
                           aria-expanded="false">
                            <div class="admin-title">
                                @Html.Label(UserSession.GetUsers.Teacher.FullName, new { @class = "item-title" })
                                <span>@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.Teacher</span>
                            </div>
                            <div class="admin-img">
                                <img src="~/Content/img/figure/admin.jpg" alt="Admin">
                            </div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <div class="item-header" style="font-size: large">
                                @Html.Label(UserSession.GetUsers.Teacher.FullName, new { @class = "item-title" })
                            </div>
                            <div class="item-content">
                                <ul class="settings-list">
                                    <li><a href="@Url.Action("Logout", "Account")"><i class="flaticon-turn-off"></i>@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.LogOut</a></li>
                                </ul>
                            </div>
                        </div>
                    </li>
                    <li class="navbar-item dropdown header-language">
                        <a class="navbar-nav-link dropdown-toggle" href="#" role="button"
                           data-toggle="dropdown" aria-expanded="false"><i class="fas fa-globe-americas"></i>@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.Language</a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <a class="dropdown-item" href="@Url.Action("Language", "Account", new { lngName = "en", view = ViewBag.ViewToShow, cont = ViewBag.ControllerToShow, layout = "Profesor" }, null)">@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.English</a>
                            <a class="dropdown-item" href="@Url.Action("Language", "Account", new { lngName = "sq", view = ViewBag.ViewToShow, cont = ViewBag.ControllerToShow, layout = "Profesor" }, null)">@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.Albanian</a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
        <!-- Header Menu Area End Here style="height: 100%; width: 100%; position:fixed;" -->
        <!-- Page Area Start Here -->
        <div class="dashboard-page-one" style="height: 100%; width: 100%;">
            <!-- Sidebar Area Start Here -->
            <div class="sidebar-main sidebar-menu-one sidebar-expand-md sidebar-color">
                <div class="mobile-sidebar-header d-md-none">
                    <div class="header-logo">
                        <a href="index-2.html"><img src="~/Content/img/logo1.png" alt="logo"></a>
                    </div>
                </div>
                <div class="sidebar-menu-content">
