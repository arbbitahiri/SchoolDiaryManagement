                            <a href="@Url.Action("Index", "Professor")" class="nav-link"><i class="flaticon-dashboard"></i><span>@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.Dashboard</span></a>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("Index", "Topic")" class="nav-link"><i class="flaticon-chat"></i><span>@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.Topic</span></a>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("Index", "Absence")" class="nav-link"><i class="flaticon-shopping-list"></i><span>@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.Absence</span></a>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("Index", "Comment")" class="nav-link"><i class="flaticon-script"></i><span>@SchoolDiarySystem.Resources.Layout.Teacher.TeacherTexts.Comment</span></a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="dashboard-content-one">
                @RenderBody()
            </div>
        </div>
